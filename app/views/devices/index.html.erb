<h1>Listing devices</h1>

<table>
  <thead>
    <tr>
      <th>Userphonenumber</th>
      <th>Oemid</th>
      <th>Os</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Createdby</th>
      <th>Modifiedby</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @devices.each do |device| %>
      <tr>
        <td><%= device.UserPhoneNumber %></td>
        <td><%= device.OEMId %></td>
        <td><%= device.OS %></td>
        <td><%= device.Lat %></td>
        <td><%= device.Lng %></td>
        <td><%= device.CreatedBy %></td>
       
        <td><%= device.ModifiedBy %></td>
       
        <td><%= link_to 'Show', device %></td>
        <td><%= link_to 'Edit', edit_device_path(device) %></td>
        <td><%= link_to 'Destroy', device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Device', new_device_path %>
