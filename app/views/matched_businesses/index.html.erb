<h1>Listing matched_businesses</h1>

<table>
  <thead>
    <tr>
      <th>Contact phone</th>
      <th>Business</th>
      <th>Isrecommended</th>
      <th>Createdby</th>
      
      <th>Modifiedby</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matched_businesses.each do |matched_business| %>
      <tr>
        <td><%= matched_business.contact_phone_id %></td>
        <td><%= matched_business.business_id %></td>
        <td><%= matched_business.IsRecommended %></td>
        <td><%= matched_business.CreatedBy %></td>
        
        <td><%= matched_business.ModifiedBy %></td>
        
        <td><%#= link_to 'Show', matched_business %></td>
        <td><%= link_to 'Edit', edit_matched_business_path(matched_business) %></td>
        <td><%#= link_to 'Destroy', matched_business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Matched business', new_matched_business_path %>
