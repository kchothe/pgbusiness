<h1>Listing contact_phones</h1>

<table>
  <thead>
    <tr>
      <th>Phonenumber</th>
      <th>Phonenumbertype</th>
      <th>Createdby</th>
      
      <th>Modifiedby</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_phones.each do |contact_phone| %>
      <tr>
        <td><%= contact_phone.PhoneNumber %></td>
        <td><%= contact_phone.PhoneNumberType %></td>
        <td><%= contact_phone.CreatedBy %></td>
       
        <td><%= contact_phone.ModifiedBy %></td>
        
        <td><%= link_to 'Show', contact_phone %></td>
        <td><%= link_to 'Edit', edit_contact_phone_path(contact_phone) %></td>
        <td><%= link_to 'Destroy', contact_phone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact phone', new_contact_phone_path %>
